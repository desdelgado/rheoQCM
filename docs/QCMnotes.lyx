\begin_layout Plain Layout

delf=-(tan(f)/f)*(1+(tan(m))^2)/(1-tan(f)*tan(m));
\end_layout

\begin_layout Plain Layout

predict=-(1/f)*(tan(m+f)-tan(m));
\end_layout

\begin_layout Plain Layout

rat=delf/predict;
\end_layout

\begin_layout Plain Layout

simplify(rat)
\end_layout

\end_inset


\end_layout

\begin_layout Plain Layout
What is the limit here for large values of 
\begin_inset Formula $r^{*}$
\end_inset

? 
\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{\Delta f_{n}^{*}}{f_{sn}}=-\frac{\tan(D_{nf}^{*})}{D_{nf}^{*}}\left[\frac{1-\left(r^{*}\right)^{2}}{1+ir^{*}\tan(D_{nf}^{*})}\right]
\]

\end_inset


\end_layout

\begin_layout Subsection
Thin Film Form:
\end_layout

\begin_layout Plain Layout
for thin films we can neglect the tangent term in the denominator and replace
 the leading tangent function with its argument to get:
\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{\Delta f_{n}^{*}}{f_{sn}}=\left(\left(r^{*}\right)^{2}-1\right)
\]

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{\Delta f_{n}^{*}}{f_{sn}}=\left(\frac{Z_{nm,0}}{Z_{nf}^{*}}\right)^{2}-1
\]

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{\Delta f_{n}^{*}}{f_{sn}}=\frac{\left(Z_{nm,0}\right)^{2}}{\rho_{f}G_{f}^{*}}-1
\]

\end_inset


\end_layout

\begin_layout Plain Layout
Further simplifications: 
\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
f_{sn}=\frac{2nf_{1}^{2}}{Z_{q}}\rho d
\]

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
-\Delta f^{*}=\frac{if_{1}\Delta Z^{*}}{\pi Z_{q}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{if_{1}\Delta Z^{*}}{\pi Z_{q}}=-\frac{2nf_{1}^{2}}{Z_{q}}\rho d
\]

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\Delta Z_{sn}^{*}=\frac{2\pi nf_{1}}{i}\rho d=2\pi inf_{1}\rho d
\]

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\Delta f_{n}^{*}=f_{sn}\frac{\left(Z_{nm,0}\right)^{2}}{\rho G^{*}}-f_{sn}
\]

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\Delta f_{n}^{*}=\frac{2nf_{1}^{2}}{Z_{q}}\rho_{f}d_{f}\frac{\left(2\pi inf_{1}\rho d\right)^{2}}{\rho_{f}G_{f}^{*}}-\frac{2nf_{1}^{2}}{Z_{q}}\rho d
\]

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\Delta f_{n}^{*}=-\frac{8\pi^{2}n^{3}f_{1}^{4}}{Z_{q}}d_{f}\frac{M_{Am}^{2}}{G_{f}^{*}}-\frac{2nf_{1}^{2}}{Z_{q}}\rho_{f}d_{f}
\]

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\[
\frac{\Delta f_{n}^{*}}{d_{f}}=-\frac{8\pi^{2}n^{3}f_{1}^{4}}{Z_{q}}{\color{red}\frac{M_{Am}^{2}}{G_{f}^{*}}}-\frac{2nf_{1}^{2}\rho_{f}}{Z_{q}}
\]

